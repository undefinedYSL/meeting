<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.administrator.testonly">

    <uses-permission android:name="android.permission.READ_CONTACTS" />
    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"

        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".Main2Activity" />
        <activity
            android:name=".Main3Activity"
            android:label="@string/title" />
        <activity android:name=".BoardroomActivity" />
        <activity android:name=".MessageActivity" />
        <activity android:name=".MaillistActivity" />
        <activity android:name=".MoreActivity" />
        <activity android:name=".HeaderActivity" />
        <activity android:name=".MeetingActivity" />
        <activity android:name=".InformActivity" />
        <activity android:name=".ExistedMeetingActivity" />
        <activity android:name=".ExistedBoardroomActivity" />
        <activity android:name=".ExistedInformActivity" />
        <activity android:name=".AddBoardroomActivity" />
        <activity android:name=".EnrollActivity" />

        <service
            android:name=".InformIntentService"
            android:exported="false" />
        <service
            android:name=".MyService"
            android:enabled="true"
            android:exported="true" />

        <activity android:name=".SearchActivity" />
        <activity android:name=".WaitingActivity"></activity>
    </application>

</manifest>